{
  "name": "electron-pdf-cms",
  "version": "1.0.0",
  "scripts": {
    "build-dev": "cross-env NODE_ENV=development webpack --env=dev --progress --profile --colors",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --env=dev",
    "electron": "electron ./dist/main.js",
    "clean": "rimraf ./dist/*",
    "test": "jest --coverage --verbose"
  },
  "repository": "https://github.com/leberjs/electron-pdf-cms.git",
  "license": "MIT",
  "jest": {
    "coverageReporters": [
      "html"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "setupTestFrameworkScriptFile": "<rootDir>/app/setupTests.js",
    "snapshotSerializers": ["enzyme-to-json/serializer"]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "concurrently": "^3.5.0",
    "copy-webpack-plugin": "^4.1.1",
    "cross-env": "^5.1.0",
    "css-loader": "^0.28.7",
    "electron": "^1.7.8",
    "electron-devtools-installer": "^2.2.1",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "enzyme-to-json": "^3.2.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "foreman": "^2.0.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "node-sass": "^4.5.3",
    "react-test-renderer": "^16.0.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "autosuggest-highlight": "^3.1.1",
    "electron-settings": "^3.1.3",
    "lunr": "^2.1.4",
    "material-ui": "next",
    "material-ui-icons": "^1.0.0-beta.17",
    "react": "^16.0.0",
    "react-autosuggest": "^9.3.2",
    "react-dom": "^16.0.0",
    "react-modal": "^3.1.0"
  }
}
